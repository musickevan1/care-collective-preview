{
  "name": "care-collective-preview",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "node scripts/update-sw-version.js",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "build:profile": "next build --profile",
    "start": "next start",
    "lint": "next lint",
    "verify": "node scripts/verify-setup.js",
    "setup:check": "node scripts/verify-setup.js",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:run": "vitest run",
    "type-check": "tsc --noEmit",
    "bundle-analyze": "npm run build:analyze && npx serve .next/analyze",
    "perf:lighthouse": "lighthouse http://localhost:3000 --chrome-flags=\"--headless\" --output=json --output-path=./lighthouse-results.json",
    "perf:test": "npm run build && npm run start & sleep 10 && npm run perf:lighthouse && killall node",
    "db:start": "supabase start",
    "db:stop": "supabase stop",
    "db:reset": "supabase db reset",
    "db:push": "supabase db push",
    "db:pull": "supabase db pull",
    "db:migration": "supabase migration new",
    "db:types": "~/.local/bin/supabase gen types typescript --project-id kecureoyekeqhrxkmjuh > lib/database.types.ts",
    "db:seed": "supabase db seed",
    "db:link": "supabase link --project-ref kecureoyekeqhrxkmjuh",
    "db:test-rls": "vitest run tests/database/rls-policies.test.ts",
    "test:database": "vitest run tests/database/",
    "db:security-audit": "node scripts/security-audit.js",
    "db:security-audit:json": "node scripts/security-audit.js --output=json",
    "db:security-audit:verbose": "node scripts/security-audit.js --verbose",
    "backup:full": "scripts/automated-backup.sh --type=all",
    "backup:schema": "scripts/automated-backup.sh --type=schema",
    "backup:data": "scripts/automated-backup.sh --type=data",
    "backup:config": "scripts/automated-backup.sh --type=config",
    "backup:verify": "scripts/automated-backup.sh --type=all --verify",
    "dr:test": "scripts/disaster-recovery-test.sh",
    "dr:test-restore": "scripts/disaster-recovery-test.sh --test-type=restore",
    "dr:test-integrity": "scripts/disaster-recovery-test.sh --test-type=backup-integrity",
    "performance:analyze": "psql -f scripts/analyze-query-performance.sql",
    "performance:scaling": "psql -f scripts/scaling-analysis.sql",
    "performance:load-test": "node scripts/performance-load-test.js",
    "monitoring:setup": "psql -f scripts/monitoring-setup.sql",
    "monitoring:daemon": "scripts/monitoring-daemon.sh",
    "monitoring:health": "scripts/monitoring-daemon.sh --dry-run",
    "workflow:setup": "scripts/setup-development-workflow.sh",
    "workflow:setup-all": "scripts/setup-development-workflow.sh --all",
    "workflow:hooks": "scripts/setup-development-workflow.sh --install-hooks",
    "quality:check": "npm run type-check && npm run lint && npm run test:run",
    "quality:fix": "npm run lint -- --fix && npm run format",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "deploy:staging": "scripts/deploy-database-changes.sh staging auto",
    "deploy:production": "scripts/deploy-database-changes.sh production manual",
    "validate-migration": "scripts/validate-migration.sh"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.57.2",
    "@types/dompurify": "^3.0.5",
    "@types/react-window": "^1.8.8",
    "@types/validator": "^13.15.2",
    "@vercel/kv": "^3.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "dompurify": "^3.2.6",
    "dotenv": "^17.2.1",
    "lucide-react": "^0.539.0",
    "next": "^14.2.32",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-pull-to-refresh": "^2.0.1",
    "react-window": "^2.2.2",
    "resend": "^6.0.2",
    "tailwind-merge": "^3.3.1",
    "tailwindcss-animate": "^1.0.7",
    "validator": "^13.15.15",
    "vaul": "^1.1.2",
    "web-vitals": "^4.2.4",
    "zod": "^4.0.17"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@next/bundle-analyzer": "^15.4.6",
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^5.0.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-next": "^15.4.6",
    "jest-axe": "^10.0.0",
    "jsdom": "^26.1.0",
    "lighthouse": "^12.2.1",
    "msw": "^2.10.5",
    "node-mocks-http": "^1.17.2",
    "serve": "^14.2.3",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4",
    "vitest-canvas-mock": "^0.3.3",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
